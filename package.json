{
  "name": "flicks",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -f ./package-lock.json && rm -f ./npm-*.log && npm cache verify",
    "devices": "instruments -s devices",
    "init": "rm -rf ./node_modules && npm install",
    "lint": "standard --fix --parser babel-eslint",
    "nuke": "npm run clean && npm run init",
    "pods": "cd ios && pod install && cd ..",
    "start": "npm run pods && concurrently 'react-native start' 'react-native run-ios --simulator=\"iPhone 8 (13.2)\"'",
    "upgrade": "npx upgradeps -n -s react-native"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "debounce": "^1.2.0",
    "native-base": "^2.13.8",
    "react": "^16.11.0",
    "react-native": "^0.60.6",
    "react-native-gesture-handler": "^1.5.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "standard": "^14.3.1"
  }
}
